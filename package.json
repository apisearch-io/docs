{
  "name": "apisearch-docs",
  "version": "0.1.0",
  "description": "Apisearch documentation site",
  "main": "index.js",
  "license": "MIT",
  "author": "Apisearch team <hello+docs@puntmig.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/apisearch-io/docs.git"
  },
  "keywords": [
    "docs",
    "documentation-website",
    "documentation-tool"
  ],
  "watch": {
    "dev": {
      "patterns": "src",
      "extensions": "js,sass,mustache,md"
    }
  },
  "scripts": {
    "server": "webpack-dev-server --config ./webpack.web.js --content-base ./docs --inline --hot --port 1234",
    "media": "cp -r ./src/media ./docs/assets/",
    "dev:website": "webpack --config ./webpack.web.js",
    "dev": "./docgen; npm run dev:website;",
    "build": "./docgen; npm run media; npm run dev:website",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "fs-path": "0.0.23",
    "highlight.js": "^9.12.0",
    "hogan.js": "^3.0.2",
    "markdown-it": "^8.4.1",
    "markdown-it-headinganchor": "^1.3.0",
    "markdown-toc": "^1.2.0",
    "node-sass": "^4.9.0",
    "npm-watch": "^0.3.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "remarkable": "^1.7.1",
    "remarkable-meta": "^1.0.1",
    "sass-loader": "^6.0.7",
    "sections": "^1.0.0",
    "style-loader": "^0.19.1",
    "uslug": "^1.0.4",
    "walk": "^2.3.13",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "apisearch-ui": "^0.1.24",
    "axios": "^0.17.1",
    "bootstrap": "4.0.0-beta.2",
    "gfm-code-blocks": "^1.0.0",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10"
  }
}
