{
  "name": "apisearch-docs",
  "version": "0.1.0",
  "description": "Apisearch documentation site",
  "main": "index.js",
  "license": "MIT",
  "author": "alexhoma <alexcm.14@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/alexhoma/docs.git"
  },
  "keywords": [
    "docs",
    "documentation-website",
    "documentation-tool"
  ],
  "watch": {
    "dev:generate": {
      "patterns": "src",
      "extensions": "js,sass,mustache,md"
    }
  },
  "scripts": {
    "clear": "rm -rf ./docs/*",
    "dev:website": "webpack --config ./webpack.web.js --progress --colors && webpack-dev-server --config ./webpack.web.js --content-base . --inline --hot --port 8080",
    "build:website": "webpack --config ./webpack.web.js",
    "dev:generate": "npm run clear; node ./bin/docgen.js; npm run dev:website;",
    "build:generate": "npm run clear; node ./bin/docgen.js; npm run build:website;",
    "generate:watch": "npm-watch"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "optimize-css-assets-webpack-plugin": "^3.2.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^2.9.7",
    "yargs": "^8.0.2"
  },
  "dependencies": {
    "bootstrap": "4.0.0-beta.2",
    "fs-path": "0.0.23",
    "highlight.js": "^9.12.0",
    "hogan.js": "^3.0.2",
    "lodash": "^4.17.4",
    "remarkable": "^1.7.1",
    "remarkable-meta": "^1.0.1",
    "walk": "^2.3.9"
  }
}
